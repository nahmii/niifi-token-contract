{
    "env": {
        "es2020": true,
        "node": true,
        "mocha": true
    },
    "parserOptions": {
        "sourceType": "script",
        "ecmaFeatures": {
            "impliedStrict": false
        }
    },
    "rules": {
        "import/no-unresolved": "off",
        "import/no-extraneous-dependencies": "off",
        "strict": ["error", "global"]
    },
    "extends": "airbnb/base"
}